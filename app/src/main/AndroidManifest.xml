<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xq.bilibilidemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BilibiliDemo">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.OtherActivity">
        </activity>
        <activity
            android:name=".activity.DemoActivity">
        </activity>
        <activity
            android:name=".activity.DemoActivityOfToolBar">
        </activity>
        <activity
            android:name=".activity.DemoActivityOfNavigationView">
        </activity>
        <activity
            android:name=".activity.DemoActivityOfFloatingActionButton">
        </activity>
        <activity
            android:name=".activity.DemoActivityOfSwipeRefreshLayout">
        </activity>
    </application>

</manifest>